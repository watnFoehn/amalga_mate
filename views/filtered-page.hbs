<h1>Your search results:</h1>

{{!-- https://balsamiq.cloud/sw2slnu/py6rvpu/r2835 --}}



<ul>
  {{#each data}}
  <br>
  <a href='/auth/profile/{{this.username}}'>
    <li>{{this.username}}</li>
  </a>
  <div class="avatar-container">
    <div class="avatar-upload">
      <div class="avatar-preview">
        <div id="imagePreview" style="background-image: url({{this.imgPath}});">
        </div>
      </div>
    </div>
  </div>
  <hr>
  {{/each}}
</ul>
